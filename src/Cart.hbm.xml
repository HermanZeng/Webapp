<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping>
	<class name="Entity.Cart" table="cart">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="user_id" type="java.lang.String" column="user_id" />
		<property name="item_id" type="java.lang.String" column="item_id" />
		<property name="numbers" type="int" column="numbers" />
	</class>
</hibernate-mapping>